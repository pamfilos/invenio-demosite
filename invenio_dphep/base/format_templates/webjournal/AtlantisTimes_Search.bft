<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<title><BFE_WEBJOURNAL_INFO var='WEBJOURNAL_NAME_INTL' /></title>
<link href="<BFE_WEBJOURNAL_INFO var='WEBJOURNAL_CSS_URL' />" rel="stylesheet" type="text/css" />
</head>

<body id='webjournal'>
<table cellspacing="0" cellpadding="0" id="container">
  <tr id="headerRow">
    <td id="headerBox"><h1 class="journalTitle"><BFE_WEBJOURNAL_INFO var='WEBJOURNAL_NAME_INTL' /></h1>
    <div id="imprint">&nbsp;</div>
    </td>
  </tr>
  <tr id="imprintRow">
    <td class="contentBox">
      <div align="right">
	<span class="header"><BFE_WEBJOURNAL_IMPRINT /></span>
      </div>
    </td>
  </tr>
  <tr id="navigationRow">
    <td class="contentBox">
      <BFE_WEBJOURNAL_MAIN_NAVIGATION />
    </td>
  </tr>
  <tr id="contentRow">
    <td class="contentBox">
    <table width="100%" border="0" cellpadding="10">
      <tr>
        <td valign="top" id="leftColumn">
	  <BFE_WEBJOURNAL_ARCHIVE />
	</td>
        <td valign="top" id="rightColumn">

	  <BFE_WEBJOURNAL_WIDGET_WHATSNEW prefix='<h2 class="rightColumnHeader">What&rsquo;s New</h2>'/>

	  <BFE_WEBJOURNAL_WIDGET_FEATURERECORD prefix='<h2 class="rightColumnHeader">In Pictures</h2>'/>

	  <BFE_WEBJOURNAL_WIDGET_WEATHER prefix='<h2 class="rightColumnHeader"><nobr>' suffix='Weather</nobr></h2>' weather_icon_only='true' />
	  <BFE_WEBJOURNAL_WIDGET_WEATHER />

          <h2 class="rightColumnHeader">Archives</h2>
          <form id="search" name="search" method="get" action="<BFE_SERVER_INFO var='CFG_SITE_URL'/>search/">
            <nobr><input type="text" name="p" id="searchField" />
            <input type="submit" name="searchButton" id="searchButton" value="Search" />
	    <input type="hidden" name="cc" value="Atlantis Times"> <!-- FIXME: make more generic -->
            </nobr>
          </form>
          </td>
      </tr>
    </table>
    </td>
  </tr>
  <tr>
    <td id="footerBox">&nbsp;</td>
  </tr>
</table>
</body>
</html>
